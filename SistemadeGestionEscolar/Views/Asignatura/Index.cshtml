@model IEnumerable<SistemadeGestionEscolar.Models.Asignatura>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Agregar asignatura", "Create")
</p>
<table class="table" id="myTable">

    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombreAsignatura)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombreAsignatura)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.asignaturaID }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.asignaturaID }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.asignaturaID })
        </td>
    </tr>
}

</table>
