@model SistemadeGestionEscolar.Models.Alumno

@{
    ViewBag.Title = "AlumnoDelete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ELIMANAR ALUMNO</h2>

<h3>Are you sure you want to delete this?</h3>
@Html.HiddenFor(a => a.Id)

<span><strong>Email:</strong></span>@Model.Email<br>
<span><strong>User Name:</strong></span>@Model.UserName<br>
<span><strong>Nombre:</strong></span>@(Model.nombre + " " + Model.apellidoPaterno + " " + Model.apellidoMaterno)<br />



        <form action="confirmarEliminar" method="post">
           
            <button type="submit" class="btn btn-primary">Confirmar</button>
            @Html.ActionLink("Regresar","Index")
        </form>
